          "schema": {
            "title": "Email",
            "type": "string"
          },
          "name": "email",
          "in": "path"
        }],
        "responses": {
          "200": {
            "description": "Successful Response",
            "content": {
              "application/json": {
                "schema": {}
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    },
    "/email": {
      "post": {
        "summary": "Add",
