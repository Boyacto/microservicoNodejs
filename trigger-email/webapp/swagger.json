            }
          }
        }
      },
      "TriggerEmail": {
        "title": "TriggerEmail",
        "required": ["from_email", "to_email", "content"],
        "type": "object",
        "properties": {
          "from_email": {
            "title": "From Email",
            "type": "string"
          },
          "to_email": {
            "title": "To Email",
            "type": "string"
          },
          "content": {
            "title": "Content",
            "type": "string"
          }
        }
      },
      "ValidationError": {
